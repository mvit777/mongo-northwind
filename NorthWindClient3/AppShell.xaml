<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NorthWindClient3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:NorthWindClient3.Pages"
    Shell.FlyoutBehavior="Flyout">
    <FlyoutItem Title="Home">
        <ShellContent
        ContentTemplate="{DataTemplate local:MainPage}"/>
    </FlyoutItem>
    <FlyoutItem Title="Products">
        <ShellContent
        ContentTemplate="{DataTemplate local:ProductsPage}"/>
    </FlyoutItem>
    <FlyoutItem Title="Orders">
        <ShellContent
        ContentTemplate="{DataTemplate local:OrdersPage}"/>
    </FlyoutItem>
</Shell>
